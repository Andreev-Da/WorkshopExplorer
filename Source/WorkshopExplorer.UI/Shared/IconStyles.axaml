<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:icons="clr-namespace:WorkshopExplorer.Shared.Views.Icons">
    <Design.PreviewWith>
        <StackPanel Margin="20">
            <icons:BackIcon/>
            <icons:CropOriginalIcon/>
            <icons:MenuIcon/>
            <icons:MoreHorizIcon/>
            <icons:MoreVertIcon/>
            <icons:SearchIcon/>
            
            <StackPanel.Styles>
                <Style Selector="StackPanel">
                    <Setter Property="Spacing" Value="10"/>
                </Style>
                <Style Selector="Path">
                    <Setter Property="Fill"></Setter>
                    <Setter Property="MaxWidth" Value="24"/>
                    <Setter Property="MaxHeight" Value="24"/>
                    <Setter Property="VerticalAlignment" Value="Center"/>
                    <Setter Property="HorizontalAlignment" Value="Center"/>
                </Style>
            </StackPanel.Styles>
        </StackPanel>
    </Design.PreviewWith>
    
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="Views/Icons/Icon.axaml"/>
                <ResourceInclude Source="Views/Icons/SearchIcon.axaml"/>
                <ResourceInclude Source="Views/Icons/MoreHorizIcon.axaml"/>
                <ResourceInclude Source="Views/Icons/CropOriginalIcon.axaml"/>
                <ResourceInclude Source="Views/Icons/MoreVertIcon.axaml"/>
                <ResourceInclude Source="Views/Icons/BackIcon.axaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Styles.Resources>
</Styles>
