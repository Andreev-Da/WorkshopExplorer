<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:bitmap="clr-namespace:WorkshopExplorer.Pages.Bitmap"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WorkshopExplorer.Pages.Bitmap.BitmapTestPage"
             x:DataType="bitmap:BitmapTestPageModel">
    <Grid RowDefinitions="Auto, *">
        <TextBlock>BitmapTestPage</TextBlock>
        <Button Grid.Row="1" Command="{Binding SwapCommand}">Swap</Button>
    </Grid>
</UserControl>
